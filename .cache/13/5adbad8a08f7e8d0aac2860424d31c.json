nter;\r\n        margin: -32px 0;\r\n    }\r\n    &__icon {\r\n        height: 115px;\r\n    }\r\n}\r\n\r\n.team {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.worker-card {\r\n    flex-basis: 33%;\r\n    &__photo {\r\n        height: 168px;\r\n        display: block;\r\n        margin: 0 auto;\r\n    }\r\n    &__name {\r\n        margin: 10px 0;\r\n        font-size: 1.5rem;\r\n        font-weight: 700;\r\n        color: $main_color;\r\n        text-align: center;\r\n    }\r\n    &__position {\r\n        margin-bottom: 65px;\r\n        font-size: 1.125rem;\r\n        font-weight: 400;\r\n        font-style: italic;\r\n        text-align: center;\r\n    }\r\n}\r\n\r\n.footer {\r\n    background-color: $main_dark;\r\n    &__container {\r\n        max-width: 540px;\r\n        margin: 0 auto;\r\n    }\r\n    &__text {\r\n        color: white;\r\n        text-align: center;\r\n        font-size: 1.125rem;\r\n        padding: 40px 0;\r\n    }\r\n}\r\n\r\n// @media screen and (max-width: 991px) {\r\n//     .header {\r\n//         background-color: $main_color;\r\n//         &__row {\r\n//             max-width: 1219px;\r\n//             margin: 0 auto;\r\n//             padding: 20px 55px;\r\n//             box-sizing: border-box;\r\n//             display: flex;\r\n//             flex-direction: column;\r\n//             justify-content: space-between;\r\n//             align-items: flex-start;\r\n//             align-content: flex-start;\r\n//         }\r\n//         .menu {\r\n//             &__list {\r\n//                 margin: 0;\r\n//                 padding-left: 0;\r\n//                 display: flex;\r\n//                 flex-direction: column;\r\n//                 align-items: flex-start;\r\n//                 list-style: none;\r\n//                 border: 2px solid #00594c;\r\n//                 border-radius: 5px;\r\n//             }\r\n//             &__item {\r\n//                 margin: 10px 0;\r\n/